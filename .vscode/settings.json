{
    "editor.fontSize": 16,
    "workbench.iconTheme": "vs-seti",
    "files.autoSave": "off",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "yaml.schemas": {
        "./schema.json": "/*"
    },
    "yaml.customTags": [
        "!file scalar",
        "!md5 scalar",
        "!atob scalar",
        "!btoa scalar",
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/tslint.json": true,
        "**/tsfmt.json": true,
        "**/tsconfig.json": true
    }
}